{
  "name": "majestic-marshmallows",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "setup": "cp -f scripts/pre-commit .git/hooks/ && chmod +x .git/hooks/pre-commit && git config submodule.recurse true && git submodule update --init --recursive && cd src/mm_ui_kit && npm run setup",
    "typecheck": "vue-tsc --noEmit",
    "typecheck:watch": "vue-tsc --noEmit --watch",
    "typecheck:vux": "vue-tsc --noEmit --project ./src/mm_ui_kit/src/vux/tsconfig.vux.json",
    "lint": "eslint --ext .js,.ts,.vue --fix src cypress",
    "lint:check": "eslint --ext .js,.ts,.vue src cypress",
    "dev": "vite --host",
    "dev:typecheck": "npm-run-all --parallel typecheck:watch dev",
    "tests:open": "cypress open -e 'TAGS=not @ignore'",
    "tests:chrome": "cypress run --browser chrome -e 'TAGS=not @ignore'",
    "tests:chrome:parallel": "cypress-parallel -s tests:chrome -d cypress/cucumber/features -t 3 -m false",
    "tests:chrome:snapshots:rebase": "cypress run --browser chrome -e 'TAGS=@snapshots and not @ignore'",
    "cucumber:report": "node cypress/cucumber/reports/cucumber-html-report.js",
    "coverage:merge": "nyc merge .nyc_output coverage.json",
    "coverage:report": "nyc report --reporter=cobertura --reporter=text-summary",
    "build": "vite build",
    "build-dev": "vite build --mode development",
    "build-staging": "vite build --mode staging",
    "generate": "vite-ssg build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@gtm-support/vue-gtm": "^3.1.0",
    "@mdi/font": "^7.3.67",
    "@unleash/proxy-client-vue": "^0.1.5",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "@vueuse/core": "^12.7.0",
    "axios": "^1.7.9",
    "dayjs": "^1.11.13",
    "dompurify": "^3.2.4",
    "i18next": "^24.2.2",
    "i18next-http-backend": "^3.0.2",
    "i18next-vue": "^5.2.0",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "oidc-client-ts": "^3.1.0",
    "pinia": "^3.0.1",
    "pinia-plugin-persistedstate": "^4.2.0",
    "set-interval-async": "^3.0.3",
    "sortablejs": "^1.15.6",
    "utility-types": "^3.11.0",
    "vite-svg-loader": "^5.1.0",
    "vue": "^3.5.13",
    "vue-chartjs": "^5.3.2",
    "vue-datepicker-next": "^1.0.3",
    "vue-multiselect": "^3.2.0",
    "vue-router": "^4.5.0",
    "vuedraggable": "^4.1.0",
    "vuetify": "^3.7.13",
    "xlsx": "^0.18.5",
    "zxcvbn-typescript": "^5.0.1"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.12.6",
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.21.0",
    "@iconify-json/ep": "^1.1.10",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/lodash": "^4.17.15",
    "@types/node": "^22.13.5",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/cli-plugin-router": "~5.0.8",
    "cypress": "^13.6.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-multi-reporters": "^1.6.4",
    "cypress-parallel": "^0.14.0",
    "cypress-visual-regression": "^4.0.0",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-vue": "^9.32.0",
    "expo-jwt": "^1.6.4",
    "multiple-cucumber-html-reporter": "^3.4.0",
    "npm-run-all": "^4.1.5",
    "openapi-typescript": "^7.6.1",
    "prettier": "^3.5.2",
    "sass": "^1.85.0",
    "typescript": "^5.7.3",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^6.1.1",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-istanbul": "^7.0.0",
    "vue-tsc": "^2.2.4"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": false,
    "stepDefinitions": "cypress/cucumber/steps",
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/cucumber/reports/json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  }
}
